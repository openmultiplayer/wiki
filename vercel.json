{
  "redirects": [
    {
      "source": "wiki.open.mp/docs/:all*",
      "destination": "https://www.open.mp/docs/:all*"
    },
    {
      "source": "/wiki",
      "destination": "/docs"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "all"
        }
      ]
    }
  ]
}
